{
  "name": "exnest",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "start": "node dist/app.js",
    "dev:health": "nodemon -r tsconfig-paths/register src/modules/health/app.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:health\"",
    "start:health": "node dist/modules/health/app.js"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.18.1",
    "concurrently": "^9.2.1",
    "tsconfig-paths": "^4.2.0"
  }
}
